--Orders item by price
SELECT *
FROM superstore
ORDER BY price;

--highest value item
SELECT MAX(price), item_name
FROM superstore;

--average price of kitchen supplies
SELECT AVG(price)
FROM superstore
WHERE category = 'Kitchen Supplies';

--total stock of electronics
SELECT SUM(stock_quantity)
FROM superstore
WHERE category = 'Electronics';

--show all items in electronics and stock of each item (extra)
SELECT item_name, stock_quantity
FROM superstore
WHERE category = 'Electronics';
